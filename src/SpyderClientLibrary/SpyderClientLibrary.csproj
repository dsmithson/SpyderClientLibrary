<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>Spyder.Client</RootNamespace>
    <Platforms>AnyCPU;x86;x64</Platforms>
    <Authors>Derek Smithson</Authors>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <WarningsAsErrors />
    <OutputPath>bin\Debug</OutputPath>
    <NoWarn />
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <WarningsAsErrors />
    <OutputPath>bin\Debug</OutputPath>
    <NoWarn />
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x86'">
    <TreatWarningsAsErrors>false</TreatWarningsAsErrors>
    <WarningsAsErrors />
    <OutputPath>bin\Debug</OutputPath>
    <NoWarn />
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>bin\Release</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Assets\128x128\Black.png" />
    <None Remove="Assets\128x128\Source.png" />
    <None Remove="Assets\32x32\Source.png" />
    <None Remove="Assets\512x512\Source.png" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Assets\128x128\Black.png" />
    <EmbeddedResource Include="Assets\128x128\Source.png" />
    <EmbeddedResource Include="Assets\32x32\Source.png" />
    <EmbeddedResource Include="Assets\512x512\Source.png" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="KnightwareCore" Version="3.1.0" />
  </ItemGroup>
</Project>